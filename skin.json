{
	"name": "WMAU",
	"version": "0.1.0",
	"author": [
		"Caddie Brain",
		"Sam Wilson"
	],
	"url": "https://www.mediawiki.org/wiki/Skin:WMAU",
	"descriptionmsg": "wmau-desc",
	"namemsg": "skinname-wmau",
	"license-name": "GPL-3.0+",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"ValidSkinNames": {
		"wmau": {
			"class": "SkinWMAU",
			"args": [ {
				"name": "WMAU",
				"responsive": true,
				"templateDirectory": "skins/WMAU/templates",
				"styles": [
					"skins.wmau",
					"skins.wmau.images"
				],
				"scripts": [
					"skins.wmau.js"
				],
				"messages": [
					"sitetitle",
					"wmau-site-full-title",
					"wmau-site-email",
					"wmau-logo-wordmark-alt",
					"wmau-logo-icon-alt"
				]
			} ]
		}
	},
	"MessagesDirs": {
		"WMAU": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.wmau": {
			"class": "ResourceLoaderSkinModule",
			"styles": {
				"less/style.less": {
					"media": "screen"
				},
				"less/mobile.less": {
					"media": "screen and (max-width: 750px)"
				}
			}
		},
		"skins.wmau.images": {
			"class": "ResourceLoaderImageModule",
			"selector": ".skin-wmau-image-{name}",
			"images": {
				"logo": "images/logo.svg",
				"logo-black-small": "images/logo-black-small.svg",
				"logo-horizontal": "images/logo-horizontal.svg"
			}
		},
		"skins.wmau.js": {
			"packageFiles": [
				"js/index.js"
			]
		}
	},
	"ResourceModuleSkinStyles": {
		"wmau": {
			"+ext.echo.styles.badge": "less/ext-echo.less",
			"+ext.WikidataPageBanner": "less/ext-wikidata-page-banner.less"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources/",
		"remoteSkinPath": "WMAU/resources/"
	},
	"AutoloadClasses": {
		"SkinWMAU": "includes/SkinWMAU.php"
	},
	"manifest_version": 2
}
